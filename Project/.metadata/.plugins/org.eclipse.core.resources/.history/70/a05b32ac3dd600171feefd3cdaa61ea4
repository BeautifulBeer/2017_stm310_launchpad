#include "DataProtocol.h"


char* BluetoothData(char str[BT_STR_MAX_LENGTH]){
	char* buffer_ = malloc(
			sizeof(BD)*2 + sizeof(CR) + sizeof(LF) + sizeof(SD) +
			sizeof(char) * BT_STR_MAX_LENGTH +
			sizeof(ED) + sizeof(LF) + sizeof(CR) + sizeof(BD)*2
			);

	return buffer_;
}
