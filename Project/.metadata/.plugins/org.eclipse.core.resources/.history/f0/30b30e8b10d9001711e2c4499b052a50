#include "config_pwm.h"

void m_Init_TIM(TIM_TypeDef* TIMx){
	TIM_TimeBaseInitTypeDef time_;
	time_.TIM_Prescaler = 20;
	time_.TIM_ClockDivision = TIM_;
	TIM_TimeBaseInit(TIMx, &time_);
}

